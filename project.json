{
  "name": "du-pipe-distance",
  "description": "Calculator for current position to nearest pipe",
  "sourcePath": "src",
  "outputPath": "out",
  "builds": {
    
    "du-pipe-distance-oo": {
      "name": "du-pipe-distance-oo",
      "slots": {
        "core": {
          "name": "core",
          "type": "core"
        }
      }
    }
    
  },
  "targets": {
    "development": {
      "name": "development",
      "handleErrors": true,
      "minify": false
    },
    "production": {
      "name": "production",
      "handleErrors": false,
      "minify": true
    }
  },
  "libs": [
    {
      "id": "AR-Library",
      "path": "libs\\AR-Library",
      "remote": {
        "git": "https://github.com/tiramon/AR-Library.git"
      }
    }
  ]
}